{
  "name": "word-copilot",
  "version": "1.0.0",
  "description": "Word Copilot - AI-powered writing assistant for Microsoft Word",
  "main": "index.js",
  "scripts": {
    "postinstall": "node scripts/generate-icons.js",
    "build": "webpack --mode production",
    "build:dev": "webpack --mode development",
    "dev": "webpack serve --mode development",
    "dev:certs": "npx office-addin-dev-certs install",
    "sideload:mac": "node scripts/sideload-mac.js",
    "start": "office-addin-debugging start manifest.xml",
    "stop": "office-addin-debugging stop manifest.xml",
    "validate": "office-addin-manifest validate manifest.xml",
    "lint": "eslint src --ext .ts,.tsx"
  },
  "dependencies": {
    "@fluentui/react-components": "^9.46.0",
    "@fluentui/react-icons": "^2.0.225",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/office-js": "^1.0.377",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^6.9.1",
    "eslint": "^8.56.0",
    "html-webpack-plugin": "^5.6.0",
    "office-addin-debugging": "^5.0.12",
    "office-addin-manifest": "^1.12.8",
    "style-loader": "^3.3.4",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
